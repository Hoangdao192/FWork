<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/chat_navigation_graph"
    app:startDestination="@id/chatListFragment">
    <fragment
        android:id="@+id/chatListFragment"
        android:name="com.uet.fwork.chat.ChatListFragment"
        android:label="fragment_chat_main"
        tools:layout="@layout/fragment_chat_main" >
        <action
            android:id="@+id/action_chatListFragment_to_chatMainFragment"
            app:destination="@id/chatMainFragment" />
    </fragment>
    <fragment
        android:id="@+id/chatMainFragment"
        android:name="com.uet.fwork.chat.ChatMainFragment"
        android:label="fragment_chat_message"
        tools:layout="@layout/fragment_chat_message" />
</navigation>